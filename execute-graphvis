export HADOOP_CLASSPATH=$HADOOP_CLASSPATH:./giraph-1.1.0-SNAPSHOT-for-hadoop-2.2.0-jar-with-dependencies.jar
$HADOOP_HOME/bin/hadoop \
org.apache.giraph.GiraphRunner \
GraphVis \
-eif CSVEdgeInputFormat \
-eip networkB.csv \
-vof SVGVertexTextOutputFormat \
-op output/graphvis \
-w 1 \
-yj giraph-1.1.0-SNAPSHOT-for-hadoop-2.2.0-jar-with-dependencies.jar
